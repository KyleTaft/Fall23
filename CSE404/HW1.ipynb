   "source": [
    "import numpy as np\n",
    "A = np.matrix([[2,1,3],\n",
    "                [1,1,2],\n",
    "                [3,2,5]])\n",
    "\n",
    "#find eigenvectors\n",
    "eigvals, eigvecs = np.linalg.eig(A)\n",
    "print(eigvecs)"
   ]
  },
